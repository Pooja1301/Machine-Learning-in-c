void parsing(char *file)
{
  int i,j;		
  for(i=1;i<=TOTAL_MOVIE;i++)
  {
    user[i].mcount = 0;
  }
  FILE *fp;
  int mmid,uuid,rid;	
  char * line = NULL;
  size_t len = 0;
  ssize_t read;
  fp = fopen(file, "r");
  if (fp == NULL) 
  {
    printf("File does not exist!");
  } 
  else 
  {
    char buf[1024];
    while (fgets(buf, 1024, fp) != NULL) 
    //while( (read = getline(&line,&len,pfile) ) != -1)
    {
      int prasent_uid=0;
      int n = 0;
      char *tok = strtok(buf, "\t");
     // printf("%zd \n",line);
     while (tok != NULL) 
     {
       if(n==0)
       {
         uuid=atoi(tok);
         prasent_uid = uuid;
         user[prasent_uid].user_id = uuid;
         // printf("%d\t ",uuid);
       }
       if(n==1)
       {
         mmid=atoi(tok);
         user[prasent_uid].watchmovie[ user[prasent_uid].mcount ] = mmid;
        // printf("%d \t ",mmid);
       }
       if(n==2)
       {
         rid=atoi(tok);
         // printf("%d\t ",rid);
         user[prasent_uid].rating[ user[prasent_uid].mcount ] = rid;
         user[prasent_uid].mcount++;
       }
       //uid[uuid].mid[mmid]=1;
       n++;
       tok = strtok(NULL,"\t");   
     }
     // printf("hello\n"); 
   }
  }
  fclose(fp);
}
